        {
          "_name": "afooter1",
          "_sourceTheme": "structurem5",
          "_customHTML": "<section class=\"footer1 structurem5\" group=\"Footers\" data-bg-video=\"{{bg.type == 'video' && bg.value.url}}\" global once=\"footers\" mbr-class=\"{'mbr-parallax-background': bg.parallax}\">\n\n    <mbr-parameters>\n        <header>Size</header>\n        <input type=\"checkbox\" title=\"Full Width\" name=\"fullWidth\">\n        <input type=\"range\" inline title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"10\" step=\"1\" value=\"1\">\n        <input type=\"range\" inline title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"10\" step=\"1\" value=\"2\">\n        <header>Show/Hide</header>\n        <input type=\"checkbox\" title=\"Logo\" name=\"showLogo\" checked>\n        <input type=\"range\" title=\"Logo Size\" inline name=\"logoSize\" min=\"3\" max=\"8\" step=\"0.1\" value=\"8\" condition=\"showLogo\">\n        <input type=\"checkbox\" title=\"Brand Name\" name=\"showBrand\">\n        <input type=\"checkbox\" title=\"Links\" name=\"showLinks\" checked>\n        <input type=\"checkbox\" title=\"Social\" name=\"showSocial\">\n        <input type=\"color\" title=\"Social Color\" name=\"socialColor\" value=\"#FFFFFF\" condition=\"showSocial\">\n        <input type=\"range\" name=\"socialCount\" title=\"Social Count\" min=\"1\" max=\"6\" step=\"1\" value=\"1\" condition=\"showSocial\">\n        <input type=\"checkbox\" title=\"Copyright\" name=\"showCopyright\" checked>\n        <input type=\"checkbox\" title=\"Border\" name=\"showBorder\" condition=\"showCopyright\">\n        <input type=\"color\" title=\"Border Color\" name=\"borderColor\" value=\"#fcdb42\" condition=\"showBorder && showCopyright\">\n        <header>Background</header>\n        <fieldset type=\"background\" name=\"bg\" parallax>\n            <input type=\"image\" title=\"Image\" value=\"@ADDONS_PATH@/structurem5/components/_images/background3.jpg\">\n            <input type=\"color\" title=\"Color\" value=\"#000000\" selected>\n            <input type=\"video\" title=\"Video\" value=\"https://vimeo.com/428046504\">\n        </fieldset>\n        <header condition=\"bg.type === 'video'\">Fallback Image</header>\n        <input type=\"image\" title=\"Fallback Image\" value=\"@ADDONS_PATH@/structurem5/components/_images/background2.jpg\" name=\"fallBackImage\" condition=\"bg.type === 'video'\">\n        <input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" condition=\"bg.type !== 'color'\" checked>\n        <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#090f10\" condition=\"overlay && bg.type !== 'color'\">\n        <input type=\"range\" inline title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.5\" condition=\"overlay && bg.type !== 'color'\">\n    </mbr-parameters>\n\n    <div class=\"mbr-fallback-image disabled\" mbr-if=\"bg.type == 'video'\"></div>\n    <div class=\"mbr-overlay\" mbr-if=\"overlay && bg.type !== 'color'\" opacity=\"{{overlayOpacity}}\" bg-color=\"{{overlayColor}}\"></div>\n    <div mbr-class=\"{'container': !fullWidth, 'container-fluid': fullWidth}\">\n        <div class=\"main-container\">\n            <div class=\"navbar-brand\" mbr-if=\"showLogo || showBrand\">\n                    <span mbr-if=\"showLogo\" class=\"navbar-logo\">\n                        <a href=\"https://mobiri.se\">\n                            <img src=\"@PROJECT_PATH@/assets/images/southern-snake-catchers-logo-1-1036x661.jpeg\" alt=\"Mobirise\" mbr-style=\"{'height': logoSize + 'rem'}\">\n                        </a>\n                    </span>\n                <span mbr-if=\"showBrand\" mbr-buttons mbr-theme-style=\"display-4\" class=\"navbar-caption-wrap\" data-toolbar=\"-mbrBtnMove,-mbrBtnAdd,-mbrBtnRemove,-iconFont\"><a class=\"navbar-caption text-white text-primary\" data-app-selector=\".navbar-caption\" href=\"https://mobiri.se\" data-app-placeholder=\"Type Text\">\n                                StructureM5\n                            </a></span>\n            </div>\n            <div class=\"list-box\" mbr-if=\"showLinks\">\n                <ul class=\"list mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".list\" data-multiline=\"true\" data-app-edit=\"full\" mbr-article>\n                    <li style=\"text-decoration:none;\" class=\"mbr-text item-wrap\" data-app-selector=\".list\">\n                        <a style=\"background-image: linear-gradient(120deg, #000000 0%, #000000 100%);\" href=\"index.html#header003-2\" class=\"text-primary\">\n                            About\n                        </a>\n                    </li>\n                    <li class=\"mbr-text item-wrap\" data-app-selector=\".list\">\n                        <a style=\"background-image: linear-gradient(120deg, #000000 0%, #000000 100%);\" href=\"index.html#features003-3\" class=\"text-primary\">\n                            Why us\n                        </a>\n                    </li>\n                    <li class=\"mbr-text item-wrap\" data-app-selector=\".list\">\n                        \n                            <a style=\"background-image: linear-gradient(120deg, #000000 0%, #000000 100%);\" href=\"index.html#features004-4\" class=\"text-primary\">\n                                Testimonials\n                            </a>\n                          </li>\n                    <li class=\"mbr-text item-wrap\" data-app-selector=\".list\">\n                            <a style=\"background-image: linear-gradient(120deg, #000000 0%, #000000 100%);\" href=\"index.html#form001-5\" class=\"text-primary\">Contact</a>\n                    </li>\n                </ul>\n            </div>\n  <!---          <div class=\"social-list\" mbr-if=\"showSocial\">\n                <div class=\"soc-item\" mbr-if=\"socialCount>0\">\n                    <span mbr-icon class=\"socicon-instagram socicon mbr-iconfont mbr-iconfont-social\"></span>\n                </div>\n                <div class=\"soc-item\" mbr-if=\"socialCount>1\">\n                    <span mbr-icon class=\"socicon-youtube socicon mbr-iconfont mbr-iconfont-social\"></span>\n                </div>\n                <div class=\"soc-item\" mbr-if=\"socialCount>2\">\n                    <span mbr-icon class=\"socicon-twitter socicon mbr-iconfont mbr-iconfont-social\"></span>\n                </div>\n                <div class=\"soc-item\" mbr-if=\"socialCount>3\">\n                    <span mbr-icon class=\"socicon-facebook socicon mbr-iconfont mbr-iconfont-social\"></span>\n                </div>\n                <div class=\"soc-item\" mbr-if=\"socialCount>4\">\n                    <span mbr-icon class=\"socicon-behance socicon mbr-iconfont mbr-iconfont-social\"></span>\n                </div>\n                <div class=\"soc-item\" mbr-if=\"socialCount>5\">\n                    <span mbr-icon class=\"socicon-vimeo socicon mbr-iconfont mbr-iconfont-social\"></span>\n                </div>\n            </div> --->\n\n            <div class=\"copyright-container\" mbr-if=\"showCopyright\">\n                <div class=\"border-item\" mbr-if=\"showBorder\"></div>\n                <p class=\"copyright mb-0 mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".copyright\" data-app-placeholder=\"Type Text\">\n                    © Copyright 2024 Southern Snake Catchers - All Rights Reserved\n                </p>\n            </div>\n        </div>\n    </div>\n</section>",
